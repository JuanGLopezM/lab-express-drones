<h2>Available drones</h2>

  {{#each response}}

    {{!-- <h4>{{this.name}}</h4> --}}
    <b><p>{{this.name}}</p></b>

    <p>-{{this.propellers}}</p>

    <p>-{{this.maxSpeed}}</p>

    <a href="/drones/{{this._id}}/edit">Editar</a>
    

    <form action='/drones/{{_id}}/delete' method="POST">
      <button type="submit">Borrar</button>
    </form>

  {{/each}}